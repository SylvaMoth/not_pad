{
  "name": "not_pad",
  "version": "0.9.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --dir"
  },
  "devDependencies": {
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0"
  },
  "build": {
    "appId": "com.notpad.app",
    "productName": "not_pad",
    "win": {
      "target": "dir",
      "signAndEditExecutable": false,
      "forceCodeSigning": false
    },
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    }
  }
}
